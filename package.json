{
  "name": "hubris-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "_moduleAliases": {
    "utilities": "utilities/index.js",
    "notion": "prisma/notion.js",
    "~database": "database",
    "~api": "api",
    "~db": "prisma",
    "~models": "prisma/models/index.js"
  },
  "_moduleDirectories": [
    "modules"
  ],
  "scripts": {
    "postinstall": "link-module-alias",
    "dev": "nodemon hubris.js -w hubris.js api/*.js",
    "start": "NODE_ENV=production node hubris.js",
    "build": "prisma generate && node scripts/imports.js && vite build",
    "database": "prisma-multischema && prisma db push",
    "sync": "node database/notion.js"
  },
  "dependencies": {
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.1",
    "@notion-render/client": "^0.0.2",
    "@notionhq/client": "^2.2.14",
    "@prisma/client": "^5.7.0",
    "@prisma/internals": "^5.7.0",
    "body-parser": "^1.20.2",
    "cli-progress": "^3.12.0",
    "color": "^4.2.3",
    "cookie-parser": "^1.4.6",
    "csv-parse": "^5.5.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "immer": "^10.0.3",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "multer": "^1.4.5-lts.1",
    "prisma": "^5.7.0",
    "prisma-import": "^1.0.4",
    "prisma-multischema": "^1.1.4",
    "radix-ui": "^1.0.1",
    "react": "^18.2.0",
    "react-async-hook": "^4.0.0",
    "react-d3-tree": "^3.6.1",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-inlinesvg": "^4.1.0",
    "react-multi-select-component": "^4.3.4",
    "react-quill": "^2.0.0",
    "react-router-dom": "^6.20.1",
    "react-select": "^5.8.0",
    "react-spinners": "^0.13.8",
    "roll": "^1.3.1",
    "use-immer": "^0.9.0",
    "uuid": "^9.0.1",
    "vite-express": "*"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "html-to-text": "^9.0.5",
    "link-module-alias": "^1.2.0",
    "nodemon": "^3.0.1",
    "vite": "^5.0.2",
    "vscode-generate-index-standalone": "^1.7.1"
  }
}
