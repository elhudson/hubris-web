{
  "name": "hubris-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "_moduleAliases": {
    "utilities": "./utilities",
    "db": "./prisma",
    "@database": "./database",
    "@models": "./prisma/models",
    "@api": "./api"
  },
  "scripts": {
    "postinstall": "link-module-alias",
    "preinstall": "command -v link-module-alias && link-module-alias clean || true",
    "dev": "nodemon hubris.js -w hubris.js ./api/*.js",
    "start": "NODE_ENV=production node hubris.js",
    "build": "prisma generate && cd ./utilities && npm link && cd .. && npm link utilities && vite build",
    "database": "prisma-multischema && prisma db push",
    "sync": "node database/notion.js"
  },
  "dependencies": {
    "@emotion/babel-plugin": "^11.11.0",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.1",
    "@notion-render/client": "^0.0.2",
    "@notionhq/client": "^2.2.14",
    "@prisma/client": "^5.7.0",
    "@prisma/internals": "^5.7.0",
    "@projectstorm/react-diagrams": "^7.0.3",
    "add-dashes-to-uuid": "^1.0.0",
    "body-parser": "^1.20.2",
    "cheerio": "^1.0.0-rc.12",
    "cli-progress": "^3.12.0",
    "color": "^4.2.3",
    "cookie-parser": "^1.4.6",
    "csv-parse": "^5.5.3",
    "d3-hierarchy": "^3.1.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "immer": "^10.0.3",
    "link-module-alias": "^1.2.0",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "module-alias": "^2.2.3",
    "multer": "^1.4.5-lts.1",
    "prisma": "^5.7.0",
    "prisma-import": "^1.0.4",
    "prisma-multischema": "^1.1.4",
    "radix-ui": "^1.0.1",
    "react": "^18.2.0",
    "react-async-hook": "^4.0.0",
    "react-d3-tree": "^3.6.1",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-inlinesvg": "^4.1.0",
    "react-markdown": "^9.0.1",
    "react-multi-select-component": "^4.3.4",
    "react-quill": "^2.0.0",
    "react-router-dom": "^6.20.1",
    "react-tree-graph": "^8.0.1",
    "reactflow": "^11.10.1",
    "reaflow": "^5.2.8",
    "remark-gfm": "^4.0.0",
    "remarkable": "^2.0.1",
    "roll": "^1.3.1",
    "roll-master": "^1.0.0",
    "ts-node": "^10.9.2",
    "turndown": "^7.1.2",
    "turndown-plugin-gfm": "^1.0.2",
    "typescript": "^5.3.3",
    "use-debounce": "^10.0.0",
    "use-immer": "^0.9.0",
    "utilities": "^1.0.6",
    "uuid": "^9.0.1",
    "vite-express": "*"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^4.2.0",
    "html-to-text": "^9.0.5",
    "nodemon": "^3.0.1",
    "vite": "^5.0.2"
